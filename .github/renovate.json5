{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
  ],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchUpdateTypes": ["patch", "minor"],
      "groupName": "all minor and patch dependencies",
    },
  ],
  "enabledManagers": [
    "gomod",
    "dockerfile",
    "github-actions",
  ],
}
